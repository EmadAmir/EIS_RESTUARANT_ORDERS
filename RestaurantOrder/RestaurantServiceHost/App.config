<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="DBCS"
         connectionString="data source=.;Integrated Security=SSPI;database=RestaurantDetails"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name ="RestaurantWCFService.RestaurantService" behaviorConfiguration="mexBehavior">
        <endpoint address="RestaurantWCFService" binding="basicHttpBinding" contract="RestaurantWCFService.IRestaurantWCFService">
      </endpoint>
      <!--<endpoint address="RestaurantService" binding="netTcpBinding" contract="RestaurantService.IRestaurantServices">
        </endpoint>
      <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"></endpoint>-->
    <host>
      <baseAddresses>
        <add baseAddress="http://localhost:5050/"/>
      <!--  <add baseAddress="net.tcp://localhost:5010/"/> -->
      </baseAddresses>
    </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
            </serviceBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>